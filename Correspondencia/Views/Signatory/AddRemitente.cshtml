@{
    ViewData["Title"] = "Agregar Firmante";
}
    
    <div class="container">
        <h1 class="align-content-center text-center"> Complejo Administrativo de Colima</h1>
        <h2 class="align-content-center text-center"> Agregar Firmante</h2>
        <!--  <form action="/" method="post">-->
        <script src="~/js/Firmantes.js"></script>
        <script src="https://ajax.googleapis.com/ajax/libs/jquery/2.1.3/jquery.min.js"></script>
        <script type="text/javascript">
        getDependencias();
        </script>
        <div class="form-row">
            <div class="form-group col-md-4">
                <label for="inputNombre">Nombre's *</label>
                <input type="text" class="form-control" id="inputNombre" placeholder="Nombre">
            </div>
            <div class="form-group col-md-4">
                <label for="inputApellidoPaterno">Apellido Paterno *</label>
                <input type="text" class="form-control" id="inputApellidoPaterno" placeholder="Apellido Paterno">
            </div>
            <div class="form-group col-md-4">
                <label for="inputApellidoMaterno">Apellido Materno *</label>
                <input type="text" class="form-control" id="inputApellidoMaterno" placeholder="Apellido Materno">
            </div>
        </div>
        <div class="form-group">
            <label for="inputEmail4">Email</label>
            <input type="email" class="form-control" id="inputEmail" placeholder="Email">
        </div>
        <div class="form-group">
            <label for="inputTelefono">Telefono</label>
            <input type="text" class="form-control" id="inputTelefono" placeholder="Numero Telefonico">
        </div>
        <div class="form-row">
            <div class="form-group col-md-6">
                <label for="inputCity">Puesto *</label>
                <input type="text" class="form-control" id="inputPuesto" placeholder="Puesto que recive">
            </div>
            <div class="form-group col-md-4">
                <label for="inputState">Dependencia Origen *</label>
                <select id="inputDependencia" class="form-control"></select>
            </div>
        </div>
        <div class="form-group">
        </div>
        <button type="submit" onclick="setFirmante();" class="btn btn-primary">Guardar</button>
        <!--   </form>-->
    </div>
   

